<div class="l-modal l-modal-edit">
    <div class="l-modal-header">
        <div class="l-modal-close">
            <shopicon-regular-close></shopicon-regular-close>
        </div>
        <label>
            Rename project
        </label>
    </div>
    <div class="l-modal-content">
        <div class="l-modal-input-wrapper">
            <label>
                <input value="" maxlength="51" class="l-modal-input" id="project_edit_input" data-project-name-reset="">
            </label>
        </div>
        <div class="l-modal-error" id="edit_project_modal_error">
            Name should have at most 50 characters.
        </div>
        <div class="l-modal-buttons">
            <div class="btn btn-2nd-level l-modal-btn-discard disable">
                Discard changes
            </div>
            <a href="#" class="btn btn-1nd-level l-modal-btn-edit-save">
                Save and exit
            </a>
        </div>
    </div>
</div>